#include <stdio.h>
#include <stdlib.h>

typedef struct node{
    int data;
    struct node **next;
} Node;

Node *tree[100005];
int number[100005] = {0};
int check(int);

int main(){
    int n, a[100005], b[100005], root[100005] = {0};
    scanf("%d", &n);
    for(int i = 1; i < n; i++){
        scanf("%d%d", &a[i], &b[i]);
        root[a[i]]++;
        if(root[b[i]]) root[b[i]]++;
    }
    for(int i = 1; i <=n; i++){
        tree[i] = (Node*)malloc(sizeof(Node));
        tree[i]->data = i;
        tree[i]->next = (Node*)malloc(sizeof(Node)*root[i]);
    }
    for(int i = 1; i < n; i++) tree[a[i]]->next[number[a[i]]++] = tree[b[i]];
    for(int i = 1; i <= n; i++){
        int count = 0;
        count = check(i);
        count += root[i];
        if(i != n) printf("%d ", count);
        else printf("%d\n", count);
    }
}

int check(int head){
    int sum = 0;
    for(int i = 0; i < number[head]; i++){
        int index = tree[head]->next[i]->data;
        if(number[index]){
            sum += number[index];
            sum += check(index);
        }
    }
    return sum;
}
